[flake8]
max-line-length = 88
extend-ignore = E203,E501,W503,W504
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    env,
    .env,
    build,
    dist,
    *.egg-info,
    .pytest_cache,
    .mypy_cache,
    checkpoints,
    data,
    logs,
    pretrained_models
per-file-ignores =
    __init__.py:F401 